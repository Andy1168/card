/* pages/Withdrawals/Withdrawals.wxss */
.withd{
  width:100%;
  text-align:center;
  padding-top:10px;
  line-height:30px; 
}
.awals{
  width: 96%;
  display: inline-block;
}
.awals view{
  float: left;
}
.awalsimg{
  width: 20%;
  text-align: center;
}
.awalsimg image{
  width: 40px;
  height: 40px;
  border: 1px dotted #909090;  
  border-radius: 50%; 
  margin:8px 0 0 0; 
}
.awalslist{
  width: 30%;
  font-size: 14px;
  border-bottom: 1px solid #eee;
}
.awalslist view{
  width: 100%;
  text-align: center;
  line-height: 28px;
}
.awalslistq{
  width: 50%;
  font-size: 14px;
  border-bottom: 1px solid #eee;
}
.awalslistq view{
  width: 100%;
  line-height: 28px;
}